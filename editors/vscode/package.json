{
    "name": "story-script",
    "displayName": "StoryScript",
    "description": "LSP for StoryScript language",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:story"
    ],
    "main": "./client.js",
    "contributes": {
        "languages": [
            {
                "id": "story",
                "aliases": [
                    "StoryScript",
                    "story"
                ],
                "extensions": [
                    ".story"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "story",
                "scopeName": "source.story",
                "path": "./syntaxes/story.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "vscode-languageclient": "^9.0.1"
    }
}
